<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

  <navigation-rule>
    <description>
      Globale Navigationsregeln, gültig für alle Pageflows
    </description>
        
    <navigation-case>
      <from-outcome>login_ok</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
        
    <navigation-case>
      <from-outcome>ware_zur_Einlagerungsliste_hinzugefuegt</from-outcome>
      <to-view-id>/einlagern.xhtml</to-view-id>
    </navigation-case>
        
    <navigation-case>
      <from-outcome>einlagerung_erfolgreich</from-outcome>
      <to-view-id>/einlagern_ok.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>auslagerung_erfolgreich</from-outcome>
      <to-view-id>/auslagern_ok.xhtml</to-view-id>
    </navigation-case>
        
    <navigation-case>
      <from-outcome>einlagerungsliste_leer</from-outcome>
      <to-view-id>/einlagern.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>auslagerungsliste_leer</from-outcome>
      <to-view-id>/auslagern.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>auslagerung_nicht_erfolgreich</from-outcome>
      <to-view-id>/auslagern_fehler.xhtml</to-view-id>
    </navigation-case>     
       
    <navigation-case>
      <from-outcome>einlagerung_nicht_erfolgreich</from-outcome>
      <to-view-id>/einlagern_fehler.xhtml</to-view-id>
    </navigation-case>
            
    <navigation-case>
      <from-outcome>ware_aus_warenkorb_gelöscht</from-outcome>
      <to-view-id>/einlagern.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>auftrag_erfolgreich</from-outcome>
      <to-view-id>/einlagern_ok.xhtml</to-view-id>
    </navigation-case>
        
        
  </navigation-rule>
</faces-config>
